{
  "manifest_version": 3,
  "name": "Simple Password Manager",
  "version": "1.0",
  "description": "A basic password manager extension",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "128": "icons/icon128.png"
    }
  }
}
